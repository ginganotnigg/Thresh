		"test:folder": "cross-env NODE_ENV=test && ts-node cmd/test-folder.ts",
		"kysely": "kysely-codegen --config-file .kysely-codegenrc.json",
		"reset-db": "cross-env NODE_ENV=development && ts-node src/reset-db.ts"
